{
  "name": "cursor-bluesky-feed-generator",
  "version": "1.0.0",
  "description": "ATProto Feed Generator Starter Kit - Cursor-based interpretation with unit tests and CI/CD",
  "main": "dist/index.js",
  "repository": "https://github.com/your-username/cursor-bluesky-feed-generator.git",
  "author": "Cursor AI Assistant",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:acceptance": "jest --selectProjects acceptance",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:unit": "jest --selectProjects unit --coverage",
    "test:coverage:acceptance": "jest --selectProjects acceptance --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "publishFeed": "ts-node scripts/publishFeedGen.ts",
    "unpublishFeed": "ts-node scripts/unpublishFeedGen.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "test:all": "./scripts/test-all.sh"
  },
  "dependencies": {
    "@atproto/api": "^0.13.28",
    "@atproto/identity": "^0.4.8",
    "@atproto/lexicon": "^0.2.2",
    "@atproto/repo": "^0.8.4",
    "@atproto/syntax": "^0.1.2",
    "@atproto/xrpc-server": "^0.7.9",
    "better-sqlite3": "^11.3.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "kysely": "^0.27.4",
    "mongodb": "^6.3.0",
    "multiformats": "^9.9.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.1.2",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "inquirer": "^12.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 18"
  }
}
